/*
 * This file is used to build a complete set of Edubuntu Dapper Drake CDs
 */

/* Packages that should really be on CD1 */
#if (CDIMAGE_INSTALL_BASE == 1)
#include <edubuntu/dapper/installer>
#endif

#if (CDIMAGE_INSTALL_BASE == 1)
#include <edubuntu/dapper/boot>
#include <edubuntu/dapper/minimal>
#include <edubuntu/dapper/standard>
#if (CDIMAGE_INSTALL == 1)
#include <edubuntu/dapper/desktop>
#include <edubuntu/dapper/server>
#include <edubuntu/dapper/ship>
#if (CDIMAGE_DVD == 1)
#include <edubuntu/dapper/supported>
#endif
#endif
#endif

#if (CDIMAGE_LIVE == 1)
#include <edubuntu/dapper/ship-live>
#endif
