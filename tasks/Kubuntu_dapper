#endif
#if (CDIMAGE_LIVE == 1)
#include <kubuntu/dapper/casper>
#endif

#if (CDIMAGE_INSTALL_BASE == 1)
#include <kubuntu/dapper/minimal>
#include <kubuntu/dapper/standard>
#if (CDIMAGE_INSTALL == 1)
#include <kubuntu/dapper/desktop>
#include <kubuntu/dapper/ship>
#if (CDIMAGE_DVD == 1)
#include <kubuntu/dapper/supported>
#endif
#endif
#endif
