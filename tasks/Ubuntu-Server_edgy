/*
 * This file is used to build a complete set of Edgy Eft CDs
 * (server edition)
 */

/* Packages that should really be on CD1 */
#if (CDIMAGE_INSTALL_BASE == 1)
#include <ubuntu-server/edgy/installer>
#endif

#if (CDIMAGE_INSTALL_BASE == 1)
#include <ubuntu-server/edgy/boot>
#include <ubuntu-server/edgy/minimal>
#include <ubuntu-server/edgy/standard>
#if (CDIMAGE_INSTALL == 1)
#include <ubuntu-server/edgy/server-ship>
#if (CDIMAGE_DVD == 1)
#include <ubuntu-server/edgy/ship>
#include <ubuntu-server/edgy/live>
#include <ubuntu-server/edgy/supported>
#endif
#endif
#endif

#if (CDIMAGE_LIVE == 1 || CDIMAGE_DVD == 1)
#include <ubuntu-server/edgy/ship-live>
#endif
