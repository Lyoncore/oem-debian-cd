/*
 * This file is used to build a complete set of Xubuntu Edgy Eft CDs
 */

/* Packages that should really be on CD1 */
#if (CDIMAGE_INSTALL_BASE == 1)
#include <xubuntu/edgy/installer>
#endif

#if (CDIMAGE_INSTALL_BASE == 1)
#include <xubuntu/edgy/boot>
#include <xubuntu/edgy/minimal>
#include <xubuntu/edgy/standard>
#if (CDIMAGE_INSTALL == 1)
#include <xubuntu/edgy/desktop>
#include <xubuntu/edgy/ship>
#if (CDIMAGE_DVD == 1)
#include <xubuntu/edgy/server>
#include <xubuntu/edgy/live>
#include <xubuntu/edgy/supported>
#endif
#endif
#endif

#if (CDIMAGE_LIVE == 1 || CDIMAGE_DVD == 1)
#include <xubuntu/edgy/ship-live>
#endif
