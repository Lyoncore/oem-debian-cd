
# PowerPC subarch 
image=/install/powerpc/vmlinux
	label=oem
	alias=oem-powerpc
	initrd=/install/powerpc/initrd.gz
	append="preseed/file=/cdrom/preseed/oem.seed quiet --"
	initrd-size=8192
	read-only

# PowerPC64 subarch
image[macrisc4]=/install/powerpc64/vmlinux
	label=oem
	alias=oem-powerpc64
	initrd=/install/powerpc64/initrd.gz
	append="preseed/file=/cdrom/preseed/oem.seed quiet --"
	initrd-size=8192
	read-only
