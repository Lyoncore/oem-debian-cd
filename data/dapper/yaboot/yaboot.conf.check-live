
# PowerPC subarch
image=/casper/filesystem.kernel-powerpc
	label=check
	alias=check-powerpc
	initrd=/casper/filesystem.initrd-powerpc
	append="boot=casper integrity-check quiet splash --"
	initrd-size=1048576
	read-only

# PowerPC64 subarch
image[macrisc4]=/casper/filesystem.kernel-powerpc64-smp
	label=check
	alias=check-powerpc64
	initrd=/casper/filesystem.initrd-powerpc64-smp
	append="boot=casper integrity-check quiet splash --"
	initrd-size=1048576
	read-only
